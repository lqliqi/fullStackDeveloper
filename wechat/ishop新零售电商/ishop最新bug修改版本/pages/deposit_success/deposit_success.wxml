<view class='success'>

  <view class='icon'>
    <image src='/images/success.png'></image>
    <view>提现申请已提交 !</view>
  </view>

  <view class='title'>
    <image src='/images/qbye.png'></image>
    <text>提现金额:{{price}}元</text>
  </view>

  <view class='type'>
    <view class='wallet' bindtap='go_wallet'>我的钱包</view>
    <view class='desposit' bindtap='go_desposit'>继续提现</view>
  </view>
</view>