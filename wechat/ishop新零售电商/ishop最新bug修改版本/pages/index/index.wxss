page {
  background: #f6f6f6;
}

.wrap {
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #f6f6f6;
  position: relative;
}

.new_header {
  width: 100%;
  height: auto;
  background: linear-gradient(to bottom, rgba(0, 0, 0, 0.2), rgba(0, 0, 0, 0));
  position: relative;
}

.header_search {
  width: 100%;
  margin: 0px auto;
  padding: 20rpx 0;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
}

.header_search .search {
  width: 94%;
  position: relative;
  margin: 0 auto;
}

.header_search .search .head {
  padding: 0 60rpx 0 10rpx;
}

.header_search .search .head>view {
  background: rgba(255, 255, 255, 0.3);
  overflow: hidden;
}

.header_search .search input {
  float: left;
  display: block;
  width: 80%;
  height: 60rpx;
  font-size: 24rpx;
  line-height: 60rpx;
  border: none;
  background: none;
  color: white;
  border-radius: 8rpx;
  outline: none;
  padding-left: 20rpx;
  min-height: 60rpx;
}

.swiper_ {
  width: 100%;
  height: 426rpx;
}

.slide-image {
  width: 100%;
  height: 426rpx;
}

.head view image {
  width: 24rpx;
  height: 24rpx;
  float: left;
  margin-top: 19rpx;
  margin-left: 15rpx;
}

.head .scan {
  width: 50rpx;
  height: 50rpx;
  left: 0;
  top: 50%;
  margin-top: -25rpx;
  float: none;
  position: absolute;
}

.head .info {
  width: 50rpx;
  height: 50rpx;
  right: 0;
  top: 50%;
  margin-top: -25rpx;
  float: none;
  position: absolute;
}

.sale_title {
  width: 94%;
  overflow: hidden;
  padding: 20rpx 3%;
  background: white;
  position: relative;
  border-top: 2rpx solid #f6f6f6;
  border-bottom: 2rpx solid #f6f6f6;
}

.sale_title .title_icon {
  width: 105rpx;
  height: 40rpx;
  float: left;
  margin-top: 8rpx;
}

.sale_title .news {
  overflow: hidden;
  margin-left: 20rpx;
  font-size: 28rpx;
  line-height: 60rpx;
  /* margin-top: 2rpx; */
  height: 60rpx;
  color: #333;
  /* position: relative */
}

.sale_title .more_icon {
  width: 14px;
  height: 14px;
  top: 50%;
  margin-top: -7px;
  right: 3%;
  position: absolute;
}

.index_sale {
  width: 100%;
  height: auto;
  background: white;
  margin-top: 10rpx;
}

.sale_title {
  width: 94%;
  overflow: hidden;
  padding: 22rpx 3%;
  background: white;
  position: relative;
  /* border-top: 2rpx solid #f6f6f6; */
}

.sale_title .title_icon {
  width: 160rpx;
  height: 38rpx;
  float: left;
}

.sale_title .title_icon_hot {
  width: 110rpx;
  height: 38rpx;
  float: left;
  margin-top: 10rpx;
}

.sale_title .time {
  margin-left: 20rpx;
  font-size: 24rpx;
  line-height: 24rpx;
  float: left;
  margin-top: 14rpx;
}

.sale_title .time text {
  background: #232323;
  color: white;
  text-align: center;
  margin: 0 6rpx;
  width: 48rpx;
  display: inline-block;
  border-radius: 4rpx;
  font-size: 24rpx;
  /* line-height: 28rpx; */
  padding-top: 5rpx;
}

.swiper_title {
  height: 410rpx;
  background: white;
}

.hot_sales {
  height: 285rpx;
  /* float: left; */
  display: inline-block;
  text-align: center;
  margin-left: 20rpx;
}

.hot_sales image {
  width: 171rpx;
  height: 137rpx;
}

.hot_sales .price {
  color: #e73c0a;
  display: block;
  font-size: 28rpx;
}

.hot_sales .original {
  color: #999;
  font-size: 24rpx;
  text-decoration: line-through;
}

.pic_index {
  width: 100%;
  background: white;
  padding: 3%;
  margin-top: 10rpx;
}

.pic_index > view {
  width: 100%;
  height: 230rpx;
  overflow: hidden;
}

.pic_index .pic1 image {
  display: block;
  width: 94%;
  min-width: 240rpx;
}

.pic_index .pic2 {
  margin-top: 5px;
}

.pic_index .l_pic {
  width: 46%;
  height: 230rpx;
  float: left;
}

.pic_index .r_pic {
  width: 46%;
  height: 230rpx;
  margin-left: 2%;
  float: left;
}

.sell_con {
  margin-top: 5px;
}

.sale_title {
  width: 94%;
  overflow: hidden;
  padding: 22rpx 3%;
  background: white;
  position: relative;
  border-top: 2rpx solid #f6f6f6;
}

.sale_title .i_tit {
  font-size: 30rpx;
  text-align: center;
  display: block;
}

.seller-menu {
  width: 100%;
  height: 188rpx;
  position: relative;
  overflow: hidden;
  background-size: 100% 100%;
}

.seller-menu .background {
  width: 100%;
  height: 188rpx;
  position: absolute;
}

.seller-menu view {
  padding: 5% 0;
  width: 50%;
  height: 116rpx;
  position: absolute;
}

.seller-menu .r {
  margin-left: 50%;
  padding: 5% 0;
  width: 50%;
  height: 116rpx;
  position: absolute;
}

.seller-menu view image {
  width: 116rpx;
  height: 116rpx;
  border-radius: 100%;
  display: block;
  float: left;
  margin-left: 10%;
}

.seller-menu .title .name {
  display: block;
}

.seller-menu .title {
  margin-left: 44%;
  width: 50%;
  float: left;
  color: #a87d48;
  font-size: 26rpx;
}

.radius_title {
  font-size: 24rpx;
  background: white;
  padding: 0 5%;
  border-radius: 4px;
  line-height: 16px;
  display: inline-block;
}

.seller-menu-more {
  width: 100%;
  overflow: hidden;
  padding-top: 3%;
  background: white;
}

.seller-menu-more > view {
  width: 25%;
  text-align: center;
  float: left;
  padding-bottom: 3%;
}

.seller-menu-more view text {
  font-size: 24rpx;
  color: white;
  background: #a47740;
  padding: 1% 10%;
  border-radius: 40rpx;
  transform: scale(0.9);
  display: inline-block;
  margin-top: 1%;
}

.seller-menu-more view image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 100%;
  margin-top: 3%;
}

.sell_con {
  margin-top: 5px;
}

.find_con {
  width: 100%;
  height: auto;
  background: white;
  margin-bottom: 10rpx;
  padding-top: 10rpx;
}

.find_con .main {
  width: 94%;
  height: auto;
  margin: 0 auto;
  position: relative;
}

.find_con .main image {
  display: block;
  width: 100%;
  height: 340rpx;
  border-radius: 4px;
}

.find_con .main > view {
  position: absolute;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}

.swipe_hot_sales {
  height: 250rpx;
}

.self-support {
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  color: white;
  background: #e73f09;
  border-radius: 8rpx;
  font-size: 24rpx;
  line-height: 36rpx;
  padding: 0 10rpx;
}

.find_con .main .name {
  color: white;
  display: inline-block;
  border-top: 2rpx solid white;
  border-bottom: 2rpx solid white;
  letter-spacing: 4rpx;
  font-size: 28rpx;
  line-height: 40rpx;
  padding: 0 10rpx;
}

.find_con .swiper-container {
  height: 210rpx;
  padding: 2% 3% 10rpx;
}

.good_shop_good {
  width: 95%;
  margin-right: 5%;
  text-align: center;
}

.find_con image {
  width: 100%;
  height: 125rpx;
  border-radius: 4px;
}

.find_con .title {
  font-size: 24rpx;
  line-height: 40rpx;
  color: #e73f09;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-top: 10rpx;
}

.list_group {
  width: 100%;
  height: auto;
  padding: 0 1%;
  overflow: hidden;
}

.list_group > view:nth-of-type(2n+1) {
  margin-left: 0;
}

.list_group > view {
  width: 49%;
  margin-left: 1%;
  float: left;
  background: white;
  margin-bottom: 1%;
  padding-bottom: 10rpx;
}

.list_group view .pic {
  width: 100%;
  position: relative;
}

.list_group view .pic image {
  display: block;
  height: 312rpx;
  width: 100%;
}

.list_group view .title {
  width: 94%;
  padding: 0 3%;
  margin: 8rpx auto;
  font-size: 24rpx;
  line-height: 40rpx;
  height: 80rpx;
  overflow: hidden;
  color: #333;
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.list_group view .price {
  width: 100%;
  padding: 0 3%;
  font-size: 14px;
  line-height: 20px;
  color: #e73f09;
}

.list_group view .price .yen {
  font-size: 24rpx;
}

.list_group view .sale_num {
  line-height: 36rpx;
  font-size: 24rpx;
  margin-right: 10%;
  text-align: center;
  color: #333;
  float: right;
}

.my-view {
  display: inline-block;
  text-align: center;
}

.my-view image {
  width: 94rpx;
  height: 94rpx;
  display: block;
  margin: 0 auto;
  margin-top: 32rpx;
}

.my-view text {
  font-size: 24rpx;
  color: #242424;
  line-height: 64rpx;
}

.sq {
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  background: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  z-index: 10;
  justify-content: center;
  align-items: center;
}

.sq > view {
  background: white;
  border-radius: 10rpx;
  width: 550rpx;
  height: auto;
  font-size: 30rpx;
}

.sq .content {
  line-height: 40rpx;
  color: #848484;
  padding: 0 50rpx;
  margin-bottom: 50rpx;
}

.sq .title {
  font-size: 40rpx;
  color: black;
  margin-left: 50rpx;
  line-height: 100rpx;
}

.sq text {
  padding: 0 20rpx;
}

.sq button{
  color: #7d9dfd;
  font-size: 36rpx;
  line-height: 100rpx;
}
