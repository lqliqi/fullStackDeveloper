page {
  background: #f6f6f6;
}

.center {
  /* width: 100%; */
  padding: 10rpx 3%;
  height: auto;
  overflow: hidden;
  background: white;
}

.center .left {
  width: 32%;
  height: 174rpx;
  float: left;
  position: relative;
}

.center .left image {
  width: 100%;
  height: 100%;
  display: block;
}

.center .left text {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  display: block;
  text-align: center;
  color: white;
  font-size: 24rpx;
  line-height: 40rpx;
  background: rgba(0, 0, 0, 0.2);
}

.center .right {
  float: left;
  width: 65%;
  margin-left: 2%;
  font-size: 32rpx;
  line-height: 1.5;
  padding: 2% 0;
  color: #4b4b4b;
}

.log .title {
  padding-left: 3%;
  background: white;
  font-size: 28rpx;
  line-height: 3;
  margin-top: 1%;
  border-bottom: 2rpx solid #f0f0f0;
}

.track-list {
  position: relative;
  width: 100%;
  background: white;
  padding-top: 2%;
}
.track-list > view > view {
  position: relative;
  border-left: 2rpx solid #e73f09;
  color: #999;
  width: 87%;
  margin-left: 4%;
  padding-left: 5%;
  padding-top: 2%;
  /* border:1px solid red; */
}
.track-list > view:first-child>view{
  padding-top: 0;
}

.track-list image {
  position: absolute;
  left: -13rpx;
  top: 20rpx;
  width: 24rpx;
  height: 24rpx;
}
.track-list>view:first-child image {
  top: 5rpx;
  z-index: 10
}
.track-list>view:first-child> view::before{
  position: absolute;
  width: 2rpx;
  height: 10rpx;
  left:-2rpx;
  top:0;
  background: #fff;
  content: ''
}
.track-list .txt {
  font-size: 26rpx;
  position: relative;
  display: block;
  color: #212121;
}

.track-list .time {
  font-size: 24rpx;
  line-height: 2;
  position: relative;
  display: inline-block;
  vertical-align: middle;
  border-bottom: 2rpx solid #f0f0f0;
  width: 100%;
  color: #999;
}

.empty {
  margin-top: 30%;
  text-align: center;
  font-size: 34rpx;
}

.empty image {
  width: 356rpx;
  height: 328rpx;
}