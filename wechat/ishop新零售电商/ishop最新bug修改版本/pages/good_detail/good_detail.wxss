.flash_sale {
  overflow: hidden;
  width: 100%;
}

.flash_sale .flash {
  width: 64%;
  padding: 10rpx 0 10rpx 3%;
  float: left;
  color: white;
  background: linear-gradient(to right, #e84109, #fea700);
}

.flash_sale .price {
  font-size: 24rpx;
  line-height: 36rpx;
}

.flash_sale .price text {
  font-size: 32rpx;
}

.flash_sale .price_spike {
  line-height: 36rpx;
  font-size: 24rpx;
}

.flash_sale .price_spike .spike {
  font-size: 20rpx;
  padding: 2rpx 10rpx 0;
  line-height: 28rpx;
  border: 2rpx solid white;
  border-radius: 12rpx;
}

.flash_sale .price_spike .original_price {
  margin-left: 10rpx;
  text-decoration: line-through;
}

.flash_sale .count_down {
  width: 33%;
  padding: 10rpx 0;
  float: left;
  background: #f6e5cf;
  color: #e73f09;
  font-size: 24rpx;
  line-height: 36rpx;
  text-align: center;
}

.flash_sale .count_down text {
  color: white;
  font-size: 24rpx;
  line-height: 36rpx;
  width: 36rpx;
  text-align: center;
  background: #e73f09;
  border-radius: 8rpx;
  display: inline-block;
}

.address_det {
  height: 600rpx;
  position: fixed;
  width: 100%;
  padding: 20rpx 0 0px;
  background: white;
  z-index: 102;
  display: block;
  bottom: 0px !important;
}

.exit {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 3%;
  top: -18px;
}

.address_det .ps {
  text-align: center;
  font-size: 32rpx;
  line-height: 80rpx;
  color: #333;
}

.address_det .address {
  padding: 0 0 3% 0;
  font-size: 28rpx;
  line-height: 48rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.address_det .address image {
  float: left;
  margin-top: 10rpx;
  width: 26rpx;
  height: 30rpx;
}

.address_det text {
  float: left;
  margin-left: 8rpx;
}

.last .address {
  display: block;
  float: left;
  width: 60%;
  color: #212121;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 10rpx;
}

.address_det .is_check {
  color: #e73f09;
}

.confirm_address {
  width: 100%;
  text-align: center;
  line-height: 90rpx;
  height: 80rpx;
  background: #e73f09;
  position: fixed;
  left: 0px;
  bottom: 0px;
  color: white;
  font-size: 32rpx;
}
