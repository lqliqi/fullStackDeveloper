page {
  background: #f6f6f6;
}

.title {
  font-size: 28rpx;
  line-height: 3;
  padding-left: 3%;
}

.pass_num {
  background: white;
  width: 94%;
  margin: 0 auto;
  height: 80rpx;
  border: 1px solid #e9e9e9;
  overflow: hidden;
  border-radius: 6px;
  display: flex;
  align-content: space-around;
}

.pass_det {
  overflow: hidden;
  width: 100%;
  height: 100%;
}

.pass_num .dotted {
  border-right: 2rpx solid #e9e9e9;
  width: 16.4%;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 80rpx;
  line-height: 80rpx;
  float: left;
}

.pass_num .none {
  border-right: 2rpx solid #e9e9e9;
  width: 16.4%;
  height: 80rpx;
  line-height: 80rpx;
  float: left;
}

.pass_num .dotted:last-child {
  border-right: none;
}

.pass_num .dotted view {
  width: 16rpx;
  height: 16rpx;
  background: #333;
  border-radius: 10rpx;
}